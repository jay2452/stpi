<p id="notice"><%= notice %></p>

<div class="container-fluid">
  <div class="col-md-3">

  </div>

  <div class="col-md-6">
    <div class="table-responsive">
       <table class="table table-hover">
         <thead>
           <h4 align="center">Downloads (<%= link_to 'Add', new_download_path %>)</h4>
         </thead>
         <tbody>
           <% @downloads.each do |download| %>
             <tr>
               <td><%= link_to download.title, download.file.url %></td>
               <td><%= link_to 'Edit', edit_download_path(download) %></td>
               <td><%= link_to 'Destroy', download, method: :delete, data: { confirm: 'Are you sure?' } %></td>
             </tr>
           <% end %>
         </tbody>
       </table>
    </div>
  </div>

  <div class="col-md-3">

  </div>
</div>
