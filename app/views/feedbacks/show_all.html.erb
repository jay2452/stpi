<% provide :title, "All Feedbacks" %>
<div class="container">
  <div class="col-md-2">

  </div>

  <div class="col-md-8">
    <% @feedbacks.each do |feedback| %>
      <div class="well">
        <b>Name:</b> <%= feedback.name %> <br>
        <b>Email:</b> <%= feedback.email %>  <br>
        <b>Subject:</b> <%= feedback.subject %> <br>
        <b>Description</b> (<small>Sent at: <%= time_ago_in_words(feedback.created_at) %> ago ..</small>) <br>
        <%= raw feedback.description %>
      </div>
    <% end %>
  </div>

  <div class="col-md-2">

  </div>
</div>
